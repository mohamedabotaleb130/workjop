@model IEnumerable<workjop.Models.JobsViewModel>

@foreach (var gr in Model)
{

<h3>@gr.JobTitle</h3> 
    <table class="table table-bordered table-hover"  >
        <tr>
            <th>
                اسم الباحث

            </th>
            <th>
                نص الرسالة
            </th>
        </tr>

        @foreach (var item in gr.Items)
        {
            <tr>
                <td>

                    @item.User.UserName
                </td>
                 <td>

                    @item.message
                </td>

                <td>

                    @item.applyData
                </td>
            </tr>


        }
    </table>

}